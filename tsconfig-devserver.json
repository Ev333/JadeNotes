{
  "compilerOptions": {
      "declaration": false,
      "module": "commonjs",
      "moduleResolution": "node",
      "removeComments": false,
      "target": "ES2017",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "noImplicitAny": false,
      "noEmitOnError": true,
      "baseUrl": ".",
      "paths": {
          "webapp/*": ["src/webapp/*"],
          "jadenotes/lib/*": ["lib/*"],
          "jadenotes/devserver/*": ["devserver/*"],
          "jadenotes/electron-app/*": ["electron-app/*"]
      }
  },
  "exclude": [
    "node_modules/**/*",
    "bower_components/**/*",
    "ignore/**/*",
    "build/**/*"
  ]
}
