{
  "compilerOptions": {
       "declaration": false,
       "module": "umd",
       "moduleResolution": "node",
       "removeComments": false,
       "target": "ES6",
       "emitDecoratorMetadata": true,
       "experimentalDecorators": true,
       "noImplicitAny": false,
       "noEmitOnError": true,
       "lib": ["ES6", "DOM", "DOM.Iterable", "ScriptHost"]
  },
  "exclude": [
    "node_modules",
    "bower_components",
    "typings",
    "build"
  ]
}




